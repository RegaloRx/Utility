              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	CSE.Id
	, CSE.CaseNumber
	, CD.Legacy_Id__c CovanceCaseId
	, CD.[Status] AS CovanceStatus
	--, CSE.[Status] AS SalesForceStatus
	, CD.Sub_Status__c AS CovanceSubStatus
	--, CSE.Sub_Status__c AS SalesForceSubStatus
	--, CSE.PAP_Eligibility_Start_Date__c AS EligibilityStartDate
	--, CSE.PAP_Eligibility_End_Date__c AS EligibilityEndDate
	, CAST(CD.PAP_Eligibility_Start_Date__c as date) AS CovanceEligibilityStartDate
	, CAST(CD.PAP_Eligibility_End_Date__c as date) AS CovanceEligibilityEndDate
	--, PAT.FirstName + ' ' + PAT.LastName
FROM Covance_DM_Otezla_CaseDetails CD
	INNER JOIN [Case] CSE
		ON CD.Legacy_Id__c = CSE.External_Id__c
	INNER JOIN Contact PAT
		ON CSE.ContactId = PAT.Id
WHERE
	CSE.[Status] = 'Pending'
	AND CD.[Status] = 'Expired'</property>
