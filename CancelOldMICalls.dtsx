                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	WhoId
	, WhatId
	, N'2nd Attempt MI' AS [Subject]
	, DATEADD(DAY, 1, CAST(GetDate() as date)) AS ActivityDate
	, N'Not Started' AS [Status]
	, N'Normal' AS [Priority]
	, (SELECT Id FROM [User] WHERE [Name] = 'Data Service Admin') AS OwnerId
	--, N'00G3h0000022xltEAA' AS OwnerId
	, N'Mailing' AS [Type]
	, N'Task' AS TaskSubtype
	, N'Outbound' AS Direction__c
	, N'04 - Missing Information' AS Mailing_Template__c
	, N'Contact Made – Still Missing Info' AS Outcome__c
	, N'Patient' AS Source__c
	, Case__c
	, MI2.[Program_Affiliation_Id__c]
FROM
	RegaloRx_Work.dbo.MI2
	INNER JOIN [Case] C
		ON MI2.WhatId = C.Id
WHERE
	C.[Status] = 'Pending'
	AND C.Sub_Status__c = 'Missing Information'</property>
