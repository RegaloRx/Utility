	ImageDetails IDET
	INNER JOIN ImageUrl IU
		ON IDET.File_Name__c = IU.[FileName]
	INNER JOIN UpsertImageDetails UIDET
		ON IDET.Legacy_Id__c = UIDET.Legacy_Id__c
